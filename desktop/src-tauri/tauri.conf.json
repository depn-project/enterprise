{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Depn Enterprise",
    "version": "0.0.0"
  },
  "tauri": {
    "security": {
      "csp": null
    },
    "allowlist": {
      "all": false,
      "shell": {
        "scope": [
          {
            "name": "wg-quick",
            "cmd": "wg-quick",
            "args": ["up", "depn"]
          }
        ],
        "all": true,
        "execute": true,
        "sidecar": true,
        "open": true
      },
      "http": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.depn",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "Depn Enterprise",
        "width": 420,
        "height": 600
      }
    ]
  }
}
